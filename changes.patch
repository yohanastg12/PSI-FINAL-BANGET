@@ -0,0 +1,24 @@
+<?php
+
+namespace App\Services;
+
+use Carbon\Carbon;
+
+class TimeService
+{
+    public function generateTimeRange($from, $to)
+    {
+        $time = Carbon::parse($from);
+        $timeRange = [];
+
+        do 
+        {
+            array_push($timeRange, [
+                'start' => $time->format("H:i"),
+                'end' => $time->addMinutes(30)->format("H:i")
+            ]);    
+        } while ($time->format("H:i") !== $to);
+
+        return $timeRange;
+    }
+}
